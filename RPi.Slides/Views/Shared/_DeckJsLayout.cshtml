@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=1024, user-scalable=no">

    <title>@ViewBag.Title</title>

    <!-- Required stylesheet -->
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/core/deck.core.css")">

    
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/goto/deck.goto.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/menu/deck.menu.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/navigation/deck.navigation.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/status/deck.status.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/hash/deck.hash.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/extensions/scale/deck.scale.css")">

    <!-- Style theme. More available in /themes/style/ or create your own. -->
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/themes/style/web-2.0.css")">

    <!-- Transition theme. More available in /themes/transition/ or create your own. -->
    <link rel="stylesheet" href="@Url.Content("~/Content/deckJs/themes/transition/horizontal-slide.css")">

    <!-- Required Modernizr file -->

    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>

</head>
<body class="deck-container">
    @RenderBody()

    <!-- deck.navigation snippet -->
    <a href="#" class="deck-prev-link" title="Previous">&#8592;</a>
    <a href="#" class="deck-next-link" title="Next">&#8594;</a>

    <!-- deck.status snippet -->
    <p class="deck-status">
        <span class="deck-status-current"></span>
        /
        <span class="deck-status-total"></span>
    </p>

    <!-- deck.goto snippet -->
    <form action="." method="get" class="goto-form">
        <label for="goto-slide">Go to slide:</label>
        <input type="text" name="slidenum" id="goto-slide" list="goto-datalist">
        <datalist id="goto-datalist"></datalist>
        <input type="submit" value="Go">
    </form>

    <!-- deck.hash snippet -->
    <a href="." title="Permalink to this slide" class="deck-permalink">#</a>

    <!-- End extension snippets. -->
    <!-- Required JS files. -->
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>

    <!-- Extension JS files. Add or remove as needed. -->
    <script src="@Url.Content("~/Content/deckJs/core/deck.core.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/hash/deck.hash.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/menu/deck.menu.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/goto/deck.goto.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/status/deck.status.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/navigation/deck.navigation.js")"></script>
    <script src="@Url.Content("~/Content/deckJs/extensions/scale/deck.scale.js")"></script>

    <!-- Initialize the deck. You can put this in an external file if desired. -->
    <script>
        $(function () {
            $.deck('.slide');
        });
    </script>
</body>
</html>
